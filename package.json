{
  "name": "runjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "mkdir -p temp/cjs temp/esm",
    "postinstall": "mkdir -p temp/cjs temp/esm && cd temp/cjs && npm init -y && cd ../esm && npm init -y && echo '{\"type\":\"module\"}' >> package.json",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "autoprefixer": "^10.4.17",
    "axios": "^1.7.9",
    "next": "14.1.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  }
}
